{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Docs",
  "colors": {
    "primary": "#1FDAFF",
    "light": "#1FDAFF",
    "dark": "#1FDAFF"},
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#131415"
    }
